{
    "compilerOptions": {
        "target": "ES2022",
        "module": "nodenext",
        "moduleResolution": "NodeNext",
        "strict": true,
        "noEmit": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "exactOptionalPropertyTypes": true,
        "useDefineForClassFields": true,
        "lib": [
            "ES2022"
        ],
        "composite": false,
        "incremental": false,
        "isolatedModules": true,
        "verbatimModuleSyntax": true
    }
}
